{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "allowIndentationTabs": true,
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "linebreak-style":"off",
        "no-unused-vars":"off"
    }
}
